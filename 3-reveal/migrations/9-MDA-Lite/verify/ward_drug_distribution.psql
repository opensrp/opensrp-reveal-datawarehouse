-- Verify 9-MDA-Lite:ward_drug_distribution on pg

BEGIN;

SET search_path TO :"schema",public;

SELECT 1/count(*)
FROM pg_matviews
WHERE matviewname = 'ward_drug_distribution';

SELECT
    id,
    supervisor_id,
    supervisor_name,
    base_entity_id,
    plan_id,
    cdd_name,
    treated_male_1_4,
    treated_male_5_14,
    treated_male_above_15,
    treated_female_1_4,
    treated_female_5_14,
    treated_female_above_15,
    total_males,
    total_females,
    total_all_genders,
    supervisor_distributed,
    received_number,
    adminstered,
    damaged,
    adverse,
    remaining_with_cdd,
    returned_to_supervisor,
    days_worked,
    average_per_day
FROM ward_drug_distribution
WHERE FALSE;

ROLLBACK;
